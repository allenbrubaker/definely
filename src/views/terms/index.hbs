<div class="container">
    <h3>Search results for <kbd>{{searchTerm}}</kbd></h3>
    <table class="table table-striped table-bordered">
        <tbody>
            {{#each terms}}
            <tr><td><a href="/terms/{{this.id}}">{{this.term}}</a></td></tr>
            {{else}}
            <tr><td>No results found</td></tr>
            {{/each}}
        </tbody>
    </table>
</div>

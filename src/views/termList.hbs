<div class="container">
    <h1>Search results</h1>
    <table class="table table-striped table-bordered">
        <tbody>
            {{#each terms}}
            <tr><td><a href="/definitions/{{this.id}}">{{this.term}}</a></td></tr>
            {{else}}
            <tr><td>No results found</td></tr>
            {{/each}}
        </tbody>
    </table>
</div>
